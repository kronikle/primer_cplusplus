#include <iostream>

using std::cout;
using std::endl;

int main(){
  int sum = 0;
  int counter = 50;
  while(counter <= 100){
    sum+=counter;
    ++counter;
  }

  cout << "sum of numbers from 50 to 100 is " << sum << std::endl;

  counter = 10;
  while(counter >= 0){
    cout << counter << " " <<;
  }
  cout << "done!" << endl;
  
  return 0;
}
